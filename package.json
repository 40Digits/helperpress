{
  "name": "wpengine-site-boilerplate",
  "version": "0.1.0",
  "description": "A starting point for WordPress projects to be hosted on WPEngine",
  "main": "Gruntfile.js",
  "scripts": {
    "test": "echo \"TODO: Add tests \" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:40digits/wpengine-site-boilerplate.git"
  },
  "author": "40Digits Interactive, LLC",
  "license": "BSD-2-Clause",
  "dependencies": {
    "grunt": "~0.4.5",
    "grunt-contrib-jshint": "~0.10.0",
    "grunt-contrib-uglify": "~0.5.0",
    "grunt-contrib-sass": "~0.7.3",
    "grunt-combine-media-queries": "~1.0.19",
    "grunt-githooks": "~0.3.1",
    "grunt-autoprefixer": "~0.8.2",
    "underscore": "~1.6.0",
    "grunt-contrib-concat": "~0.5.0",
    "grunt-composer": "~0.4.0",
    "execSync": "~1.0.1-pre"
  },
  "devDependencies": {
    "grunt-contrib-watch": "~0.6.1"
  },
  "config": {
    "urls": {
      "branch": "url"
    },
    "wp": {
      "theme_slug": "wp-theme",
      "plugins": [
        "akismet",
        "wordpress-seo"
      ]
    },
    "assets_dir": "wp-theme/assets/",
    "css_dir": "wp-theme/",
    "browser_support": {
      "ie": "9"
    },
    "subtrees": {
      "40d-sass": {
        "path": "wp-theme/assets/_src/sass",
        "url": "git@bitbucket.org:40digits/40d-semantic-sass-structure.git",
        "branch": "master"
      }
    }
  }
}
